#search the AUR only with pacaur
alias pacaur='pacaur -a'

#color ascii video
alias vidcolor='mplayer -ao alsa -vo caca -geometry 1280x720 -quiet $argv 2>/dev/null'

#bw ascii vide
alias vidbw='mplayer -ao alsa -vo aa -geometry 1280x720 -quiet $argv 2>/dev/null'

alias dotfiles='/usr/bin/git --git-dir=/home/ben/.dotfiles --work-tree=/home/ben'
